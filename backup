import React, { Component } from 'react';
import {
  AppRegistry,
  StyleSheet,
  Text,
  View,
  Image,
} from 'react-native';

import Swiper from 'react-native-swiper';//图片轮播组件，需要安装加载


var Dimensions = require('Dimensions');
var ScreenWidth = Dimensions.get('window').width;
var ScreenHeight = Dimensions.get('window').height;

export default class EducationPlatform extends Component {
  render() {
    return (
        <View></View>
    );
  }
}

const styles = StyleSheet.create({
  container: {
    flex: 1,
    alignItems: 'center',
    backgroundColor: '#F5FCFF',
  },
  PicsStyle:{
    width:ScreenWidth,
    height:ScreenHeight*0.23,


    }
});

AppRegistry.registerComponent('EducationPlatform', () => EducationPlatform);
